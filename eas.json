{
  "cli": {
    "version": ">= 16.9.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal"
    },
    "preview": {
      "distribution": "internal"
    },
    "production": {
      "autoIncrement": true
    },
    "production-cn": {
      "extends": "production",
      "env": {
        "EXPO_PUBLIC_BRAND": "cn"
      },
      "prebuildCommand": "node scripts/prebuild.js cn",
      "ios": {
        "bundleIdentifier": "metropolis.co.uk.constructionnews"
      },
      "android": {
        "package": "metropolis.co.uk.constructionnews"
      }
    },
    "production-nt": {
      "extends": "production",
      "env": {
        "EXPO_PUBLIC_BRAND": "nt"
      },
      "prebuildCommand": "node scripts/prebuild.js nt",
      "ios": {
        "bundleIdentifier": "metropolis.net.nursingtimes"
      },
      "android": {
        "package": "metropolis.net.nursingtimes"
      }
    }
  },
  "submit": {
    "production": {},
    "production-cn": {},
    "production-nt": {}
  }
}
