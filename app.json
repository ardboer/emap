{
  "expo": {
    "name": "Nursing Times",
    "owner": "emap-publishing",
    "slug": "emap",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./brands/nt/assets/icon.png",
    "scheme": "emap",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "metropolis.net.nursingtimes"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./brands/nt/assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "metropolis.net.nursingtimes"
    },
    "updates": {
      "url": "https://u.expo.dev/17cbc49f-0d9b-4eb4-a9ff-4b3981b607bb"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./brands/nt/assets/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./brands/nt/assets/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "17cbc49f-0d9b-4eb4-a9ff-4b3981b607bb"
      },
      "brandConfig": {
        "dynamicName": true,
        "dynamicIcon": true,
        "dynamicSplash": true,
        "dynamicColors": true,
        "brand": "nt",
        "brandName": "Nursing Times",
        "bundleIdentifier": "metropolis.net.nursingtimes"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    }
  }
}
